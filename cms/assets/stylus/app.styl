/**
 * Avatar */
.avatar
  position: relative
  width: 75px
  height: 75px
  border-radius: 50%
  &.primary
    background-color: transparentify($primary, 0.2)
  &.secondary
    background-color: transparentify($secondary, 0.2)
  &.success
    background-color: transparentify($success, 0.2)
  &.cancel
    background-color: transparentify($cancel, 0.2)
  &.warning
    background-color: transparentify($warning, 0.2)
  &.info
    background-color: transparentify($info, 0.2)
  &.dark
    background-color: transparentify($gray, 0.2)
  .overlap
    position: absolute
    top: 50%
    left: 0
    right: 0
