@import 'fonts'

$primary = #00AAAD
$secondary = #F2A02A
$black = #111
$white = #FFF
$info = #438EFF
$cancel = #EA5455
$gray = #7A7A7A
$gray_v2 = #F6F6F6
$gray_v3 = #C3C3C3
$gray_v4 = #E2E2E2
$success = #5cbf69
$warning = #ffc800

html
  -webkit-text-size-adjust: none
  -webkit-font-smoothing: subpixel-antialiased
  backface-visibility: hidden
  brandon_regular(18px, false)

body
  &::-webkit-scrollbar
    width: 3px
    background-color: $gray_v2
  &::-webkit-scrollbar-track
    box-shadow: none
  &::-webkit-scrollbar-thumb
    background-color: $primary
    border: 6px solid transparentify($black, 0)

::-webkit-scrollbar-track
  -webkit-box-shadow: inset 0 0 6px transparentify($black, 0.1)
  background-color: $gray_v2
  border-radius: 10px
::-webkit-scrollbar
  width: 3px
  height: 5px
  background-color: $gray_v2
::-webkit-scrollbar-thumb
  border-radius: 10px
  background-color: $primary

body
  overflow-x: hidden
  background-color: $gray_v2
  &.cms
  &.no_click
    pointer-events: none
  &.no_scroll
    overflow-y: hidden

*,
*:before,
*:after
  user-select: none
  box-sizing: border-box
  margin: 0

button
  outline: none

input,
input:before,
input:after,
textarea,
textarea:before,
textarea:after,
select,
select:before,
select:after
  user-select: initial

input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button
  -webkit-appearance: none
  margin: 0

textarea
  resize: vertical

a
  text-decoration: none

h1,
h2,
h3,
h4,
h5
  font-weight: normal

img
  height: auto
  vertical-align: middle

table
  width: 100%
  border-collapse: collapse

.hidden
  display: none

.o_hidden
  position: absolute
  opacity: 0
  visibility: hidden
  z-index: -1

.none
  pointer-events: none

.quill-editor iframe,
.quill-editor video
  pointer-events: none

@page
  size: landscape
  margin: 0cm

@media print
  body
    margin: 0.75cm
    -webkit-print-color-adjust: exact

svg,
span
  vertical-align: middle

.pointer
  cursor: pointer

.mb
  margin-bottom: 30px
  &.seventy
    margin-bottom: 70px
  &.twenty
    margin-bottom: 20px !important
  &.ten
    margin-bottom: 10px !important
.mr
  margin-right: 30px
  &.ten
    margin-right: 10px
  &.twenty
    margin-right: 20px !important
.ml
  margin-left: 30px
  &.ten
    margin-left: 10px
  &.twenty
    margin-left: 20px !important
.nmb
  margin-bottom: 0 !important
.nmr
  margin-right: 0 !important

.editor
  border: 1px solid $gray
  background-color: $white
  .ql-toolbar.ql-snow
    border: none
    border-bottom: 1px solid $gray
  .ql-container.ql-snow
    border: none
    border-top: 1px solid $gray
  .ql-editor
    padding: 15px
    brandon_regular(16px, false)

.scrollable_content
  height: 100%
  max-height: 300px
  overflow-y: auto
  overflow-x: hidden

/**
 * Root */
.__db
  position: relative
  .swallower
    position: sticky
    top: 0
    z-index: 1
    height: 85px
    width: 100%
    pointer-events: none
    background: -webkit-gradient(linear,left top,left bottom,color-stop(41%,#fff),color-stop(95%,hsla(0,0%,100%,.11)),to(hsla(0,0%,100%,0)))
  &.full
    #header
      max-width: calc(100% - 130px)
      transition: .3s ease-in-out
    #footer
      max-width: calc(100% - 70px)
      transition: .3s ease-in-out
    #dashboard
      margin-left: 70px
      transition: .3s ease-in-out
    .form
      .buttons
        &.fixed
          max-width: calc(100% - 70px)
          transition: .3s ease-in-out
  #dashboard
    position: relative
    padding: 30px 30px 60px
    margin-left: 260px
    transition: .3s ease-in-out
    &.error
      .wrapper
        display: flex
        flex-flow: column wrap
        align-items: center
        justify-content: center
        height: calc(100vh - 205px)
        .icon
          img
            width: 75px
        .title
          margin-top: 10px
          brandon_bold(26px, true)
        .subtitle
          margin: 10px 0 20px
          brandon_regular(18px, false)

/**
 * Login */
#login
  position: relative
  height: 100vh
  background-color: $primary
  .wrapper
    position: absolute
    top: 50%
    left: 0
    right: 0
    width: 100%
    max-width: 1280px
    margin: 0 auto
    display: flex
    flex-flow: row wrap
    justify-content: flex-start
    border-radius: 10px
    transform: translateY(-50%)
    box-shadow: 0 0 30px transparentify($black, 0.3)
    .left
      flex: 0 0 60%
      img
        border-radius: 10px 0 0 10px
        width: 100%
        height: 450px
        object-fit: cover
    .right
      border-radius: 0 10px 10px 0
      flex: 1 0 5%
      position: relative
      background-color: $white
      .overlay
        position: absolute
        top: 50%
        left: 0
        right: 0
        padding: 30px
        transform: translateY(-50%)
        h2
          brandon_medium(32px, false)
          color: $black
        h3
          brandon_light(18px, false)
          color: $gray
          margin: 10px 0 20px
        .action
          margin-top: 30px

/**
 * Header */
#header
  position: fixed
  top: 30px
  z-index: 2
  right: 30px
  width: 100%
  max-width: calc(100% - 320px)
  display: flex
  flex-flow: row wrap
  align-items: center
  justify-content: space-between
  border-radius: 5px
  background-color: $white
  box-shadow: 0 0 30px transparentify($black, 0.1)
  transition: .3s ease-in-out
  .header_nav
    display: flex
    flex-flow: row wrap
    align-items: center
    justify-content: space-between
    padding: 0 20px
    .title
      brandon_bold(18px, true)
      color: $secondary
      letter-spacing: 0.25px
      padding-right: 10px
    #breadcrumb
      padding-left: 10px
      border-left: 1px solid #E2E2E2
  .header_select
    position: relative
    border-left: 1px solid #E2E2E2
    user-select: none
    &::before
      content: ''
      position: absolute
      top: 50%
      right: calc(0% + 15px)
      width: 0
      cursor: pointer
      border-left: 7px solid transparent
      border-right: 7px solid transparent
      border-top: 7px solid $primary
      transform: translateY(-50%) rotateX(0deg)
      transition: .3s ease-in-out
    .header_user
      padding: 10px 40px 10px 15px
      display: flex
      flex-flow: row wrap
      align-items: center
      cursor: pointer
      img
        display: inline-block
        margin-right: 5px
        width: 33px
        height: 33px
        border-radius: 50%
        object-fit: cover
        object-position: center
        border: 2px solid $secondary
      .user_picture
        position: relative
        text-transform: uppercase
        width: 35px
        height: 35px
        background-color: $primary
        border-radius: 50%
        margin-right: 10px
        .overlay
          position: absolute
          top: 50%
          left: 0
          right: 0
          text-align: center
          transform: translateY(-50%)
          color: $white
          brandon_regular(14px, false)
      .user_name
        brandon_regular(16px, false)
        color: $black
    .user_select
      position: absolute
      right: 0
      top: calc(100% + 30px)
      z-index: 1
      width: 300px
      border-radius: 5px
      background-color: $white
      opacity: 0
      visibility: hidden
      box-shadow: 15px 25px 40px transparentify($black, 0.05)
      transition: all .2s ease-in-out
      a
        text-decoration: none
        outline: none
        display: block
      .select_header
        display: flex
        flex-flow: row wrap
        align-items: center
        justify-content: space-between
        padding: 20px
        background-color: $secondary
        border-radius: 5px 5px 0 0
        .select_info
          .header_name
            brandon_regular(16px, false)
            color: $white
          .header_role
            brandon_light(14px, false)
            color: #F8F8FB
            margin-top: 5px
        .header_studio
          .action_btn
            flex: 0 0 auto
      .select_footer
        display: flex
        justify-content: space-between
        .select_item
          position: relative
          brandon_regular(14px, false)
          color: $black
          padding: 10px 15px
          transition: .3s ease-in-out
          &.account
            span
              padding-left: 10px
          &.logout
            span
              padding-right: 10px
          &:hover
            color: $primary
            transition: .3s ease-in-out

    &.active
      &::before
        transform: translateY(-50%) rotateX(180deg)
        transition: .3s ease-in-out
      .user_select
        top: 100%
        visibility: visible
        opacity: 1
        transition: all .2s ease-in-out

/**
 * Sidebar */
#sidebar
  position: fixed
  top: 0
  left: 0
  z-index: 7
  width: 100%
  max-width: 260px
  height: 100vh
  background-color: $white
  box-shadow: 0 0 30px transparentify($black, 0.1)
  border-right: 5px solid $secondary
  transition: .3s ease-in-out
  &.floating
    max-width: 65px
    .top
      h2
        position: absolute
        top: -9999999px
        left: -9999999px
        z-index: -2
        brandon_bold(0px, false)
      .toggle
        opacity: 0
        visibility: hidden
        transition: .1s ease-in-out
    .bottom
      .list
        .item
          .link
            brandon_regular(0px, false)
            span
              position: absolute
              top: -9999999px
              left: -9999999px
              z-index: -1
            &.parent
              &::after
                opacity: 0
                visibility: hidden
                transition: .3s ease-in-out
          .sub
            .sub_list
              .sub_item
                .sub_link
                  brandon_regular(0px, false)
                  span
                    position: absolute
                    top: -9999999px
                    left: -9999999px
                    z-index: -1
    &.hovered
      max-width: 260px
      transition: .3s ease-in-out
      .top
        justify-content: flex-start
        h2
          position: static
          left: 0
          z-index: 1
          brandon_bold(16px, false)
        .toggle
          opacity: 1
          visibility: visible
      .bottom
        .list
          .item
            .link
              brandon_regular(16px, false)
              justify-content: flex-start
              span
                position: static
                left: 0
                z-index: 1
              &.parent
                &::after
                  opacity: 1
                  visibility: visible
                  transition: .3s ease-in-out
            .sub
              .sub_list
                .sub_item
                  .sub_link
                    brandon_regular(16px, false)
                    justify-content: flex-start
                    span
                      position: static
                      left: 0
                      z-index: 1
  .top
    position: relative
    padding: 20px 7px
    display: flex
    flex-flow: row wrap
    align-items: center
    justify-content: flex-start
    transition: .3s ease-in-out .3s
    img
      width: 50px
      height: auto
      transition: .3s ease-in-out .3s
    h2
      padding-left: 10px
      brandon_bold(16px, false)
      color: $gray
      transition: .3s ease-in-out .3s
    .toggle
      position: absolute
      top: 50%
      right: 10px
      opacity: 1
      z-index: 2
      visibility: visible
      transform: translateY(-50%)
      transition: .3s ease-in-out .3s
      svg
        color: $primary
  .shadow
    position: absolute
    margin-top: -20px
    margin-left: -5px
    z-index: 2
    height: 60px
    width: 100%
    pointer-events: none
    -webkit-filter: blur(5px)
    filter: blur(5px)
    background: -webkit-gradient(linear,left top,left bottom,color-stop(41%,#fff),color-stop(95%,hsla(0,0%,100%,.11)),to(hsla(0,0%,100%,0)))
  .bottom
    height: 100%
    overflow-y: auto
    padding: 30px 10px 20px
    .list
      list-style: none
      padding: 0
      margin: 0
      .item
        margin-bottom: 5px
        &:last-child
          margin-bottom: 0
        &.toggled
          .link
            background-color: $gray_v2
            transition: .3s ease-in-out
            & ~ .sub
              opacity: 1
              transition: .3s ease-in-out
            &.parent
              &::after
                transform: translateY(-50%) rotateX(180deg)
                transition: .3s ease-in-out
        .link
          display: flex
          flex-flow: row wrap
          align-items: center
          justify-content: flex-start
          position: relative
          z-index: 1
          padding: 7.5px 9px
          brandon_regular(16px, false)
          color: $gray
          border-radius: 5px
          box-shadow: 0 0 20px transparent
          background-color: transparent
          transition: .3s ease-in-out
          &.parent
            &.exact,
            &.active
              color: $gray
              box-shadow: 0 0 20px transparent
              padding: 7.5px 10px
              background-color: $gray_v2
              transition: .3s ease-in-out
              svg
                &.icon
                  path
                    opacity: 1
                    fill: $primary
                    transition: .3s ease-in-out
              &::before
                opacity: 0
                visibility: hidden
                transition: .3s ease-in-out
            &::after
              content: ''
              pointer-events: none
              position: absolute
              top: 50%
              right: 10px
              z-index: 1
              border-top: 7px solid $primary
              border-left: 7px solid transparent
              border-right: 7px solid transparent
              transform: translateY(-50%) rotateX(0deg)
              transition: .3s ease-in-out
          .link_icon
            position: inherit
            z-index: 1
            pointer-events: none
            flex: 0 0 auto
            padding-bottom: 3px
          svg
            position: inherit
            z-index: 1
            &.icon
              path
                opacity: 1
                transition: .3s ease-in-out
          span
            pointer-events: none
            display: inline-block
            padding-left: 10px
            position: inherit
            z-index: 1
            transform: translateX(0)
            transition: .3s ease-in-out
          &::before
            content: ''
            position: absolute
            pointer-events: none
            top: 0
            left: 0
            right: 0
            bottom: 0
            opacity: 0
            z-index: 0
            visibility: hidden
            background-color: $primary
            border-radius: 5px
            transition: .3s ease-in-out
          &:hover
            padding: 7.5px 10px 7.5px 15px
            background-color: $gray_v2
            transition: .3s ease-in-out
          &.active,
          &.exact
            color: $white
            box-shadow: 0 0 20px transparentify($primary, 0.5)
            transition: .3s ease-in-out
            svg
              &.icon
                path
                  fill: $white
                  transition: .3s ease-in-out
            &::before
              opacity: 1
              visibility: visible
              transition: .3s ease-in-out
        .sub
          display: block
          height: 0px
          overflow: hidden
          opacity: 0
          transition: .3s ease-in-out
          .sub_list
            list-style: none
            padding: 0
            margin: 0
            .sub_item
              margin-bottom: 5px
              &:last-child
                margin-bottom: 0
              .sub_link
                display: flex
                flex-flow: row wrap
                align-items: center
                justify-content: flex-start
                position: relative
                padding: 7.5px 15px
                brandon_regular(16px, false)
                color: $gray
                border-radius: 5px
                box-shadow: 0 0 20px transparent
                transition: .3s ease-in-out
                .sub_link_icon
                  position: relative
                  z-index: 1
                  flex: 0 0 auto
                  padding-bottom: 3px
                svg
                  width: 10px
                  height: 10px
                  position: inherit
                  z-index: 1
                  &.icon
                    circle
                      opacity: 1
                      transition: .3s ease-in-out
                span
                  display: inline-block
                  padding-left: 10px
                  position: inherit
                  z-index: 1
                  transform: translateX(0)
                  transition: .3s ease-in-out
                &::before
                  content: ''
                  position: absolute
                  top: 0
                  left: 0
                  right: 0
                  bottom: 0
                  opacity: 0
                  z-index: 0
                  visibility: hidden
                  background-color: $primary
                  border-radius: 5px
                  transition: .3s ease-in-out
                &:hover
                  padding: 7.5px 10px 7.5px 20px
                  transition: .3s ease-in-out
                &.exact,
                &.active
                  color: $white
                  box-shadow: 0 0 20px transparentify($primary, 0.5)
                  transition: .3s ease-in-out
                  svg
                    &.icon
                      circle
                        stroke: $white
                        transition: .3s ease-in-out
                  &::before
                    opacity: 1
                    visibility: visible
                    transition: .3s ease-in-out

/**
 * Footer */
#footer
  position: fixed
  bottom: 0
  z-index: 2
  right: 0
  width: 100%
  max-width: calc(100% - 260px)
  display: flex
  flex-flow: row wrap
  align-items: center
  padding: 10px 30px
  background-color: $primary
  justify-content: space-between
  transition: .3s ease-in-out
  .label
    color: $white
    brandon_regular(14px, false)

/**
 * Breadcrumb */
#breadcrumb
  &.left
    .list
      justify-content: flex-start
  &.right
    .list
      justify-content: flex-end
  &.center
    .list
      justify-content: center
  .list
    list-style: none
    padding: 0
    margin: 0
    display: flex
    flex-flow: row wrap
    align-items: center
    justify-content: flex-start
    .item
      display: flex
      flex-flow: row wrap
      align-items: center
      .link
        color: $black
        brandon_regular(14px, true)
        &.nuxt-link-active
          color: $primary
        &.nuxt-link-exact-active
          color: $black
        svg
          display: block
      .separator
        color: $gray_v3
        padding: 0 5px
        brandon_regular(24px, false)

/**
 * Alert */
#alert
  &.primary
    background-color: transparentify($primary, 0.2)
    .wrapper
      p
        color: $primary
  &.secondary
    background-color: transparentify($secondary, 0.2)
    .wrapper
      p
        color: $secondary
  &.success
    background-color: transparentify($success, 0.2)
    .wrapper
      p
        color: $success
  &.cancel
    background-color: transparentify($cancel, 0.2)
    .wrapper
      p
        color: $cancel
  &.warning
    background-color: transparentify($warning, 0.2)
    .wrapper
      p
        color: $warning
  &.info
    background-color: transparentify($info, 0.2)
    .wrapper
      p
        color: $info
  &.dark
    background-color: transparentify($gray, 0.2)
    .wrapper
      p
        color: $gray
  .wrapper
    padding: 10px
    border-radius: 5px
    p
      brandon_medium(16px, false)

/**
 * Toast */
#toast_wrapper
  position: fixed
  top: 0
  right: 0
  width: 400px
  height: 100vh
  z-index: 4
  background-color: transparent
  padding: 20px
  list-style: none
  overflow-y: auto
  overflow-x: hidden
  pointer-events: none
  &::-webkit-scrollbar-track
    -webkit-box-shadow: inset 0 0 6px transparentify($black, 0.1)
    background-color: $gray_v2
    border-radius: 10px
  &::-webkit-scrollbar
    width: 5px
    height: 10px
    background-color: $gray_v2
  &::-webkit-scrollbar-thumb
    border-radius: 10px
    background-color: $primary
  #toast
    position: relative
    right: 0
    margin-bottom: 20px
    transform: translateX(0px)
    animation: right_slide 1s
    .close
      position: absolute
      top: -3px
      right: -3px
      display: inline-block
      padding: 5px 10px
      opacity: 1
      border-radius: 5px
      brandon_regular(20px, false)
      background-color: $primary
      color: $white
      box-shadow: 0 0 10px transparentify($black, 0.2)
      z-index: 2
      line-height: 1
      pointer-events: auto
      transition: .3s ease-in-out
      &:hover
        top: -6px
        right: -6px
    .wrapper
      background-color: $white
      border-left: 5px solid $primary
      box-shadow: 0 0 30px transparentify($black, 0.2)
      padding: 20px
      p
        color: $primary
        brandon_medium(18px, false)
    &.secondary
      .close
        background-color: $secondary
      .wrapper
        border-left: 5px solid $secondary
        p
          color: $secondary
    &.success
      .close
        background-color: $success
      .wrapper
        border-left: 5px solid $success
        p
          color: $success
    &.cancel
      .close
        background-color: $cancel
      .wrapper
        border-left: 5px solid $cancel
        p
          color: $cancel
    &.warning
      .close
        background-color: $warning
      .wrapper
        border-left: 5px solid $warning
        p
          color: $warning
    &.info
      .close
        background-color: $info
      .wrapper
        border-left: 5px solid $info
        p
          color: $info
    &.dark
      .close
        background-color: $gray
      .wrapper
        border-left: 5px solid $gray
        p
          color: $gray
    &.last-child
      margin-bottom: 0

/**
 * Tabs */
#tabs
  .parent
    ul
      list-style: none
      padding: 0
      margin: 0
      display: flex
      flex-flow: row wrap
      align-items: flex-start
      justify-content: flex-start
      li
        padding: 15px 20px
        brandon_bold(14px, true)
        color: $gray
        border-bottom: 5px solid $gray
        background-color: $white
        transition: .3s ease-in-out
        &.active
          color: $white
          border-bottom: 5px solid $secondary
          background-color: $primary
          transition: .3s ease-in-out
        &:hover
          color: $primary
          border-bottom: 5px solid $primary
          background-color: transparentify($primary, 0.2)
          transition: .3s ease-in-out
          &.active
            color: $white
            border-bottom: 5px solid $secondary
            background-color: $primary
            transition: .3s ease-in-out

/**
 * Collapse */
.collapse
  &.toggled
    .item_top
      &.top_box
        padding-bottom: 20px
        transition: .3s ease-in-out
      .arrow
        top: calc(50% - 10px)
        transform: translateY(-50%) rotateX(180deg)
        transition: .3s ease-in-out
    .item_bottom
      height: 100%
      opacity: 1
      visibility: visible
      transition: .3s ease-in-out
  .item_top
    position: relative
    &.top_box
      padding-bottom: 0
      transition: .3s ease-in-out
    .arrow
      content: ''
      position: absolute
      top: 50%
      right: 0
      border-left: 10px solid transparent
      border-right: 10px solid transparent
      border-top: 10px solid $secondary
      transform: translateY(-50%) rotateX(0deg)
      transition: .3s ease-in-out
  .item_bottom
    height: 0px
    opacity: 0
    visibility: hidden
    transition: .3s ease-in-out

/**
 * Buttons */
.button
  display: inline-block
  color: $white
  brandon_medium(14px, true)
  padding: 10px 15px
  border-radius: 10px
  box-shadow: 0 0 20px transparent
  text-align: center
  transform: translateY(0)
  transition: .3s ease-in-out
  &.rounded
