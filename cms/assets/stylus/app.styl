#filter
  position: absolute
  left: 0
  z-index: 1024
  .wrapper
    position: fixed
    top: 0
    left: 0
    z-index: 1050
    width: 100%
    height: 100%
    outline: 0
    .content
      position: relative
      z-index: 1041
      width: auto
      max-width: 450px
      height: 100%
      overflow-y: auto
      margin: 0 0 0 auto
      background-color: $white
      box-shadow: 0 0 30px transparentify($black, 0.2)
      transform: translateX(0)
      animation: filter_slide 1s
      .close
        position: absolute
        top: 22px
        right: 20px
        display: inline-block
        padding: 5px 10px
        opacity: 1
        border-radius: 5px
        brandon_regular(20px, false)
        background-color: $primary
        color: $white
        box-shadow: 0 0 10px transparentify($black, 0.2)
        z-index: 2
        line-height: 1
        transform: scale(1)
        transition: .3s ease-in-out
        &:hover
