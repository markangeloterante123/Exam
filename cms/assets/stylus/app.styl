      transition: .3s ease-in-out
      td
        padding: 15px
        brandon_regular(16px, false)
        color: $black
        &:first-child
          border-radius: 5px 0 0 5px
        &:last-child
          border-radius: 0 5px 5px 0
        &.upper
          text-transform: uppercase
        &.cap
          text-transform: capitalize
        &.bg
          span
            color: $white
            brandon_bold(14px, false)
            padding: 5px
            border-radius: 5px
        &.primary
