/**
 * Layout */
.box_parent
  display: flex
  flex-flow: row wrap
  justify-content: space-between
  &.three_fifty
    .box
      &.left,
      &.middle
        flex: 0 0 19%
      &.right
        flex: 0 0 58%
  &.three_seventy
    .box
      &.left
        flex: 0 0 24%
      &.right
        flex: 0 0 74%
  &.half
    .box
      &.left,
      &.right
        flex: 0 0 49%
  &.full
    .box
      flex: 0 0 100%
.box
  padding: 20px
  border-radius: 5px
  background-color: $white
  box-shadow: 0 0 30px transparentify($black, 0.1)
  .top_box
    padding-bottom: 20px
    &.inline
      display: flex
      flex-flow: row wrap
      align-items: center
      justify-content: space-between
    h2
      brandon_medium(16px, true)
      color: $gray
    .description
      padding-top: 10px
      *
        brandon_regular(16px, false)
        color: $black
      b
        brandon_bold(16px, false)
        color: $secondary
        background-color: transparentify($secondary, 0.25)
        padding: 0 5px 2.5px
  .bottom_box
    border-bottom: 2px solid $gray_v4
    margin-bottom: 20px
    &.scrollable
      height: 100%
      max-height: 600px
      overflow-y: auto
      overflow-x: hidden
    &.pd
      padding: 20px 0
    &.npb
      padding-bottom: 0
    &:last-child
      margin-bottom: 0
      border-bottom: 2px solid transparent
    .group_header
      brandon_bold(14px, true)
      color: $secondary
      margin-bottom: 10px
      &.inline
        display: flex
        flex-flow: row wrap
        align-items: center
        justify-content: space-between
    .box_group
      padding: 10px
      box-shadow: 0 0 30px transparentify($black, 0.1)
    .box_group_inline
      display: flex
      flex-flow: row wrap
      align-items: center
      justify-content: flex-start
.box_inline
  display: flex
  flex-flow: row wrap
  justify-content: space-between
  .box
    flex: 0 0 calc(50% - 15px)

/**
 * Columns */
.columns
  margin: 0 -10px
  .wrapper
    display: flex
    flex-flow: row wrap
    align-items: flex-start
    justify-content: flex-start
    &.image
      .column
        padding: 0
    &.four
      .column
        flex: 0 0 calc(25% - 20px)
        margin: 0 10px 20px
    .column
      text-align: center
      border: 2px solid $primary
      padding: 30px
      border-radius: 5px
      transition: .3s ease-in-out
      &:hover
        background-color: $gray_v2
      .title
        brandon_medium(20px, false)
        margin-top: 10px
        color: $black
      .count
        brandon_bold(50px, true)
        color: $primary
      img
        width: 100%
        height: auto
        border-radius: 5px

/**
 * Loader */
.loader
  position: fixed
  top: 0
  left: 0
  bottom: 0
  right: 0
  z-index: 99955
  background-color: transparentify($black, 0.2)
  .wrapper
    position: absolute
    top: 50%
    left: 0
    right: 0
    text-align: center
    margin: 0 auto
    width: 250px
    transform: translateY(-50%)
    .icon
      margin-bottom: 10px
    .dots
      display: inline-block
      position: relative
      width: 80px
      height: 80px
      div
        animation: rotate 1.2s cubic-bezier(0.5, 0, 0.5, 1) infinite
        transform-origin: 40px 40px
        &::after
          content: " "
          display: block
          position: absolute
          width: 7px
          height: 7px
          border-radius: 50%
          background-color: $primary
          margin: -4px 0 0 -4px
        &:nth-child(1)
          animation-delay: -0.036s
          &::after
            top: 63px
            left: 63px
        &:nth-child(2)
          animation-delay: -0.072s
          &::after
            top: 68px
            left: 56px
        &:nth-child(3)
          animation-delay: -0.108s
          &::after
            top: 71px
            left: 48px
        &:nth-child(4)
          animation-delay: -0.144s
          &::after
            top: 72px
            left: 40px
        &:nth-child(5)
          animation-delay: -0.18s
          &::after
            top: 71px
            left: 32px
        &:nth-child(6)
          animation-delay: -0.216s
          &::after
            top: 68px
            left: 24px
        &:nth-child(7)
          animation-delay: -0.252s
          &::after
            top: 63px
            left: 17px
        &:nth-child(8)
          animation-delay: -0.288s
          &::after
            top: 56px
            left: 12px

#dialog
  position: absolute
  left: 0
  z-index: 1024
  &.overlap
    z-index: 9999
  &.centered
    .wrapper
      .content
        top: 50%
        transform: translateY(-50%)
        animation: centered_slide 1s
  .wrapper
    position: fixed
    top: 0
    left: 0
    z-index: 1050
    width: 100%
    height: 100%
    outline: 0
    overflow-x: hidden
    overflow-y: auto
    .content
      position: relative
      z-index: 1041
      width: auto
      max-width: 600px
      border-radius: 5px
      margin: 0 auto
      background-color: $white
      box-shadow: 0 0 30px transparentify($black, 0.2)
      transform: translateY(50px)
      animation: basic_slide 1s
      &.extended
        max-width: 900px
      &.secondary
        .close
          background-color: $secondary
        .content_top
          color: $secondary
      &.success
        .close
          background-color: $success
        .content_top
          color: $success
      &.cancel
        .close
          background-color: $cancel
        .content_top
          color: $cancel
      &.warning
        .close
          background-color: $warning
        .content_top
          color: $warning
      &.info
        .close
          background-color: $info
        .content_top
          color: $info
      &.dark
        .close
          background-color: $gray
        .content_top
          color: $gray
      .close
        position: absolute
        top: -3px
        right: -3px
        display: inline-block
        padding: 5px 10px
        opacity: 1
        border-radius: 5px
        brandon_regular(20px, false)
        background-color: $primary
        color: $white
        box-shadow: 0 0 10px transparentify($black, 0.2)
        z-index: 2
        line-height: 1
        transition: .3s ease-in-out
        &:hover
          top: -6px
          right: -6px
          transition: .3s ease-in-out
      .content_top
        border-radius: 5px 5px 0 0
        background-color: transparentify($black, 0.05)
        padding: 10px 20px
        brandon_medium(20px, false)
        color: $primary
      .content_middle
        padding: 10px 20px
        brandon_light(16px, false)
        color: $black
        .disclaimer
          .violator
            brandon_light(16px, false)
            color: $black
      .content_bottom
        border-top: 2px solid transparentify($black, 0.05)
        text-align: right
        padding: 10px 20px
  .backdrop
    position: fixed
    top: 0
    z-index: 1040
    width: 100vw
    height: 100vh
    background-color: transparentify($black, 0.2)

/**
 * Forms */

/**
 * Switch */
#switch
  display: flex
  flex-flow: row wrap
  align-items: center
  &.disabled
    opacity: 0.5
    pointer-events: none
  &.cancel
    label
      background-color: $cancel
      border: 2px solid $cancel
      transition: .3s ease-in-out
  &.toggled
    &.secondary
      label
        background-color: $secondary
        border: 2px solid $secondary
        transition: .3s ease-in-out
    &.success
      label
        background-color: $success
        border: 2px solid $success
        transition: .3s ease-in-out
    &.warning
      label
        background-color: $warning
        border: 2px solid $warning
        transition: .3s ease-in-out
    &.info
      label
        background-color: $info
        border: 2px solid $info
        transition: .3s ease-in-out
    &.dark
      label
        background-color: $gray
        border: 2px solid $gray
        transition: .3s ease-in-out
    label
      background-color: $primary
      border: 2px solid $primary
      transition: .3s ease-in-out
      &::before
        left: calc(100% - 22px)
        transition: .3s ease-in-out
  label
    cursor: pointer
    position: relative
    display: inline-block
    width: 50px
    height: 25px
    background-color: $gray_v3
    border: 2px solid $gray_v3
    border-radius: 50px
    transition: .3s ease-in-out
    &::before
      content: ''
      position: absolute
      top: 0
      left: 0
      width: 22px
      height: 100%
      border-radius: 50%
      background-color: $white
      z-index: 0
      transition: .3s ease-in-out
  input
    position: absolute
    left: 0
    z-index: -1
    opacity: 0
    visibility: hidden
  span
    display: inline-block
    padding-left: 7.5px
    brandon_regular(14px, true)
    color: $gray
/**
 * Input */
.form_group
  #input
    display: block
    width: 100%

.fade
  animation: fade 1s

@keyframes fade
  0% {
    opacity: 0
  }
  100% {
    opacity: 1
  }

@keyframes rotate
  0% {
    transform: rotate(0deg)
  }
  100% {
    transform: rotate(360deg)
  }

@keyframes basic_slide
  from {
    opacity: 0
    transform: translateY(0)
  }
  to {
    opacity: 1
    transform: translateY(50px)
  }

@keyframes centered_slide
  from {
    opacity: 0
    transform: translateY(-100%)
  }
  to {
    opacity: 1
    transform: translateY(-50%)
  }

@keyframes right_slide
  from {
    opacity: 0
    transform: translateX(100px)
  }
  to {
    opacity: 1
    transform: translateX(0)
  }

@keyframes filter_slide
  from {
    opacity: 0
    transform: translateX(450px)
  }
  to {
    opacity: 1
    transform: translateX(0)
  }

.page-enter-active,
.page-leave-active
  transition: all .3s ease-in-out

.page-enter,
.page-leave-to
  opacity: 0

.fade-enter-active,
.fade-leave-active
  transition: all .3s ease-in-out

.fade-enter,
.fade-leave-to
  opacity: 0

.right_slide-enter-active,
.right_slide-leave-active
  transition: all .3s ease-in-out

.right_slide-enter,
.right_slide-leave-to
  opacity: 0
  transform: translateX(100px)

.slide-enter-active,
.slide-leave-active
  transition: all .2s ease-in-out

.slide-enter,
.slide-leave-to
  opacity: 0
  transform: translateY(-20px)
