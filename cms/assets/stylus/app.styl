            span
              background-color: $primary
        &.secondary
          &.bg
            span
              background-color: $secondary
        &.info
          &.bg
            span
              background-color: $info
        &.cancel
          &.bg
            span
              background-color: $cancel
        &.success
          &.bg
            span
              background-color: $success
        &.warning
          &.bg
            span
              background-color: $warning
        &.gray
          &.bg
            span
              background-color: $gray
        .name
          brandon_medium(16px, false)
          text-decoration: underline
          color: $primary
          transition: .3s ease-in-out
          &:hover
            text-shadow: 0px 0px 15px transparentify($primary, 0.5)
        &.thumb
          .wrapper
            display: flex
            flex-flow: row wrap
            align-items: center
            img
              width: 35px
              height: 35px
              border-radius: 50%
              object-fit: cover
              object-position: center
            .image
              position: relative
              background-color: $secondary
              brandon_regular(14px, true)
              border-radius: 50%
              color: $white
              width: 35px
              height: 35px
              .overlay
                position: absolute
                top: 50%
                left: 0
                right: 0
                text-align: center
                transform: translateY(-50%)
            .name
              flex: 1 0 5%
              padding-left: 10px
        &.buttons
          display: table-cell
          .wrapper
            display: flex
