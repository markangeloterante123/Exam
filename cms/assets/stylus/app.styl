      color: $black
      appearance: none
  .next
    margin-right: 10px
  .prev
    margin-left: 10px
  .next,
  .prev
    display: flex
    flex-flow: row wrap
    align-items: center
    background-color: $primary
    border: 1px solid $primary
    padding: 5px
    transition: .3s ease-in-out
    .icon
      stroke: $white
      fill: transparent
      transition: .3s ease-in-out
    &:hover,
    &:focus
      background-color: $white
      .icon
        stroke: $primary
