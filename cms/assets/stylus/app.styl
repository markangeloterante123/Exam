        color: $gray
        z-index: 0
        opacity: 0.5
        transform: translateY(-50%)
        transition: .3s ease-in-out
      .input,
      .field-input
        &.disabled
          & ~ .dd
            border-top: 7px solid $gray
            transform: translateY(calc(-50% + 10px))
          & ~ label
            brandon_medium(12px, true)
            color: $gray
            top: 0
            transition: .3s ease-in-out
        &.filled,
        &:focus
          border-bottom: 1px solid $primary
