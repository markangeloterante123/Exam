        padding: 20px
  .backdrop
    position: fixed
    top: 0
    z-index: 1040
    width: 100vw
    height: 100vh
    background-color: transparentify($black, 0.2)

/**
 * Data List */
.table
  border-collapse: separate
  border-spacing: 0 15px
  table-layout: auto
  &.striped
    tbody
      tr
        border-radius: 5px
        box-shadow: 0 0 20px 0 transparent
        transition: .3s ease-in-out
        &:nth-child(odd)
          background-color: transparentify($primary, 0.2)
        &:nth-child(even)
          background-color: transparent
  thead
    &.sticky
      tr
        th
