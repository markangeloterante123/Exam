      color: $black
      appearance: none
      transition: .3s ease-in-out
      &.picker
        height: 45px
      &.search
        background-image: url('data:image/svg+xml;base64,')
        background-position: calc(100% - 5px) center
        background-repeat: no-repeat
      &.disabled
        pointer-events: none
        cursor: not-allowed
        background-color: transparentify($black, 0.1)
      &:focus
        border-bottom: 1px solid $primary
        transition: .3s ease-in-out
    &.floating
      label
        position: absolute
        pointer-events: none
        top: 50%
        left: 0
